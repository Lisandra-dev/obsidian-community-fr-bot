{
  "name": "obsidian-community-fr-bot",
  "version": "1.0.0",
  "description": "Bot for the Obsidian Community FR Discord server",
  "main": "dist/index.js",
  "repository": "github:lisandra-dev/obsidian-community-fr-bot",
  "scripts": {
    "init": "pnpm i && node install.js",
    "build": "tsc",
    "test": "tsc --noEmit --skipLibCheck",
    "prestart": "pnpm run build",
    "start": "pm2 start dist/src/index.js --name obsidian-bot --log-date-format=\"YYYY-MM-DD HH:mm Z\"",
    "stop": "pm2 stop obsidian-bot",
    "prerestart": "pnpm run build",
    "restart": "pm2 restart obsidian-bot",
    "delete": "pm2 delete obsidian-bot",
    "dev": "ts-node-dev --respawn -- src/index.ts",
    "dev:prod": "cross-env ENV=production ts-node-dev --respawn -- src/index.ts",
    "lint:fix": "eslint --fix --ext .ts src/"
  },
  "keywords": [],
  "author": "lisandra-dev",
  "license": "GNU GPLv3",
  "dependencies": {
    "@discordjs/rest": "^2.2.0",
    "@lisandra-dev/eslint-config": "^1.1.4",
    "@types/parse-color": "^1.0.3",
    "ansi-colors": "^4.1.3",
    "color-parse": "^2.0.0",
    "cron": "^3.1.6",
    "cron-validator": "^1.3.1",
    "cross-env": "^7.0.3",
    "dedent": "^1.5.1",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.55.0",
    "i18next": "^23.10.1",
    "nodemon": "^3.0.2",
    "octokit": "^3.1.2",
    "ts-dedent": "^2.2.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.2"
  }
}